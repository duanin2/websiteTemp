<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link rel="stylesheet" href="/styles/style.css">
<meta name="twitter:card" content="summary">
<title>{{ page.title }} - {{ site.title }}</title>
<meta property="og:title" content="{{ page.title }}">
<meta property="og:site_name" content="{{ site.title }}">
<meta property="twitter:title" content="{{ page.title }}">
<link rel="icon" type="image/svg+xml" href="/images/icon.svg">
<link rel="apple-touch-icon" sizes="180x180" href="/images/icon-180px.png">
<link rel="apple-touch-icon" sizes="167x167" href="/images/icon-167px.png">
<link rel="apple-touch-icon" sizes="152x152" href="/images/icon-152px.png">
<link rel="apple-touch-icon" sizes="120x120" href="/images/icon-120px.png">
<meta name="author" content="duanin2">
<meta name="fediverse:creator" content="@duanin2@mastodon.arch-linux.cz">
{% capture url %}{{ site.base_url }}/{{ page.permalink }}{% endcapture %}
<link href="{{ url }}" rel="canonical">
<meta property="og:url" content="{{ url }}">
<meta property="og:locale" content="{{ page.data.language }}">
{% if page.description.size > 0 %}
<meta name="description" content="{{ page.description }}">
<meta property="og:description" content="{{ page.description }}">
<meta property="twitter:description" content="{{ page.description }}">
{% endif %}
<link href="https://creativecommons.org/licenses/by-sa/4.0/?ref=chooser-v1" rel="license">
<link href="{{ site.base_url }}" rel="author">